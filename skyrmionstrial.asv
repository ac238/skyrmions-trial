
N=50; % num of pts in each direction

% create bounds of graph
xlow=-5;
ylow=-5;
xhigh=5;
yhigh=5;
[xx,yy]=meshgrid(linspace(xlow,xhigh,N),linspace(ylow,yhigh,N));

% initialize skyrmion according to QHMF 35
n=1;
z_0=0;
lambda=1;
omega=((x+y*j)/lambda)^n
uu=sin(xx);
vv=5*cos(yy);

% plot
quiver(xx,yy,uu,vv)
axis([xlow xhigh ylow yhigh])